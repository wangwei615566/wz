	<script type="text/javascript">
		function downLoad() {
			if (window.browser.iPhone || window.browser.ipad
					|| window.browser.ios) {
				iosDownload();
			} else {
				// 安卓3.4.1以下版本 由于is_share固定为1，特殊处理，
				androidDownload();
			}
		}
		function iosDownload() {
			if (!window.browser.wx) {
